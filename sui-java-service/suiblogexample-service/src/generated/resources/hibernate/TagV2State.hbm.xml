<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.test.suiblogexample.domain.tagv2">
  <class name="org.test.suiblogexample.domain.tagv2.AbstractTagV2State$SimpleTagV2State" table="tag_v2">
    <id name="id" length="66" column="id">
      <generator class="assigned" />
    </id>
    <version name="offChainVersion" column="off_chain_version" type="long" />
    <property name="name">
      <column name="name" length="100" />
    </property>
    <property name="version">
      <column name="version" sql-type="DECIMAL(20,0)" />
    </property>
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="active">
      <column name="active" />
    </property>
    <property name="deleted">
      <column name="deleted" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
